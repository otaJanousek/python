x, y, z = 1, 2, -3

if (pow(y, 2) - (2 * x * 2 * z)) == 0:
    print("1:", -y / (2 * x))
elif (pow(y * 1, 2) - (pow(2 * x, 2) * z)) > 0:
    d = (pow(y ** 2, 1) - (4 * (1 * x) * (1 * z))) ** (2 * (2 ** -2))
    e = ((y * y) - (2 ** 3 * (pow(1, 2) * x * z / 2))) ** 0.5
    print("1: ", (-y + d) / (2 * x),
          "2: ", (-y - e) / (2 * x)
          )
else:
    print("Oops")
